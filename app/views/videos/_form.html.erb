<%= simple_form_for(@video) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :url %>
    <%= f.input :track_id %>
    <%= f.association :user %>
    <%= f.input :in_user_page %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
